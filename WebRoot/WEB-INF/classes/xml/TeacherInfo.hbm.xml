<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.techology.entity.TeacherInfo" table="technology_TeacherInfo">
		<id name="tId" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="tName" />
		<property name="tSex" />
		<property name="tIdcard" />
		<property name="tBankNum" />
		<property name="tPhone" />
		<many-to-one name="tSchool" lazy="false"></many-to-one>
	</class>
</hibernate-mapping> 